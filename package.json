{
  "name": "@ryanarora/discord-bot-template",
  "private": true,
  "description": "A template for my Discord bots",
  "main": "./src/index.js",
  "scripts": {
    "build:clean": "rimraf ./build && pnpm build",
    "build:watch": "swc ./src --out-dir ./build -w",
    "genv": "generate-env-types --pretty true",
    "genv:example": "generate-env-types --pretty true --definition-path \"\"",
    "genv:types": "generate-env-types --pretty true --example-path\"\"",
    "preinstall": "npx -y only-allow pnpm",
    "lint": "pnpm run lint:sort-package-json && pnpm run lint:prettier",
    "lint:check": "prettier --check .",
    "lint:prettier": "prettier --write .",
    "lint:sort-package-json": "sort-package-json",
    "prepare": "husky install",
    "start:dev": "cross-env NODE_ENV=development node ./build/index.js",
    "start:prod": "cross-env NODE_ENV=production node ./build/index.js",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "build": "swc ./src --out-dir ./build"
  },
  "dependencies": {
    "@swc/helpers": "^0.3.2",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.3",
    "discord.js": "^13.6.0",
    "dotenv": "^16.0.0",
    "fs-extra": "^10.0.0",
    "regenerator-runtime": "^0.13.9",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@commitlint/types": "^16.0.0",
    "@ryanarora/generate-env-types": "git+https://github.com/ryanArora/generate-env-types.git#1.1.1",
    "@swc/cli": "^0.1.55",
    "@swc/core": "^1.2.138",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.17",
    "@types/prettier": "^2.4.4",
    "chokidar": "^3.5.3",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.3",
    "only-allow": "^1.0.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "sort-package-json": "^1.54.0",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  }
}
